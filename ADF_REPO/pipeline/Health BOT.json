{
	"name": "Health BOT",
	"properties": {
		"activities": [
			{
				"name": "Rest Copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['started_at']"
								},
								"sink": {
									"name": "Started_at",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['completed_at']"
								},
								"sink": {
									"name": "Completed_at",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "GUID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['respondent']['id']"
								},
								"sink": {
									"name": "Emp_id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['respondent']['name']"
								},
								"sink": {
									"name": "Emp_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['respondent']['first_name']"
								},
								"sink": {
									"name": "First_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['respondent']['last_name']"
								},
								"sink": {
									"name": "Last_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['answers'][0]['question_id']"
								},
								"sink": {
									"name": "Question_id",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "['answers'][0]['question_text']"
								},
								"sink": {
									"name": "Question_text",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['answers'][0]['content']"
								},
								"sink": {
									"name": "Answer",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['responses']"
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable5",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}