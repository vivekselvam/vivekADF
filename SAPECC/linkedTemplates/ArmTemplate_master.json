{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SAPECC"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"AzureSqlDatabase3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase3'"
		},
		"Office365_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'Office365'"
		},
		"RestService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService1'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Azure-Vault-Dev.vault.azure.net/"
		},
		"Office365_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "vivek02@smrt.com.sg"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://surveybot.io"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vivek02"
		},
		"SAPECC_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "100"
		},
		"SAPECC_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vivek02"
		},
		"SAPECC_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myerpabapdev.smrt.com.sg"
		},
		"SAPECC_properties_typeProperties_systemNumber": {
			"type": "string",
			"defaultValue": "00"
		},
		"SAPED1_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "100"
		},
		"SAPED1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vivek02"
		},
		"SAPED1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "myerpabapdev.smrt.com.sg"
		},
		"SAPED1_properties_typeProperties_systemNumber": {
			"type": "string",
			"defaultValue": "00"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SAPECC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"AzureSqlDatabase3_connectionString": {
						"value": "[parameters('AzureSqlDatabase3_connectionString')]"
					},
					"Office365_servicePrincipalKey": {
						"value": "[parameters('Office365_servicePrincipalKey')]"
					},
					"RestService1_password": {
						"value": "[parameters('RestService1_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Office365_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Office365_properties_typeProperties_servicePrincipalId')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "SAPECC_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SAPECC_properties_typeProperties_clientId": {
						"value": "[parameters('SAPECC_properties_typeProperties_clientId')]"
					},
					"SAPECC_properties_typeProperties_userName": {
						"value": "[parameters('SAPECC_properties_typeProperties_userName')]"
					},
					"SAPECC_properties_typeProperties_server": {
						"value": "[parameters('SAPECC_properties_typeProperties_server')]"
					},
					"SAPECC_properties_typeProperties_systemNumber": {
						"value": "[parameters('SAPECC_properties_typeProperties_systemNumber')]"
					},
					"SAPED1_properties_typeProperties_clientId": {
						"value": "[parameters('SAPED1_properties_typeProperties_clientId')]"
					},
					"SAPED1_properties_typeProperties_userName": {
						"value": "[parameters('SAPED1_properties_typeProperties_userName')]"
					},
					"SAPED1_properties_typeProperties_server": {
						"value": "[parameters('SAPED1_properties_typeProperties_server')]"
					},
					"SAPED1_properties_typeProperties_systemNumber": {
						"value": "[parameters('SAPED1_properties_typeProperties_systemNumber')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'SAPECC_ArmTemplate_0')]"
			]
		},
		{
			"name": "SAPECC_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'SAPECC_ArmTemplate_1')]"
			]
		}
	]
}